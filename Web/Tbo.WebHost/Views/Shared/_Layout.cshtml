@using Core
<!DOCTYPE html>
<html lang="ru">
  <head>
    <title>@ViewBag.Title</title>

    <meta charset="utf-8">
    <meta name="description" content="">

    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1"> -->

    <meta property="og:image" content="path/to/image.jpg">
    <link rel="shortcut icon" href="~/Content/img/favicon/favicon.ico" type="image/x-icon">
    <link rel="apple-touch-icon" href="~/Content/img/favicon/apple-touch-icon.png">
    <link rel="apple-touch-icon" sizes="72x72" href="~/Content/img/favicon/apple-touch-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="114x114" href="~/Content/img/favicon/apple-touch-icon-114x114.png">

    <link rel="stylesheet" href="~/Content/custom.min.css">

    <!-- Chrome, Firefox OS and Opera -->
    <meta name="theme-color" content="#000">
    <!-- Windows Phone -->
    <meta name="msapplication-navbutton-color" content="#000">
    <!-- iOS Safari -->
    <meta name="apple-mobile-web-app-status-bar-style" content="#000">
  </head>

  <body class="sidebar-mini skin-green-light">

    @RenderBody()

    <script src="~/Scripts/libs.min.js"></script>

    @if (Settings.TestEnvironment)
    {
      <script src="~/Scripts/scripts.js"></script>
    }
    else
    {
      <script src="~/Scripts/scripts.min.js"></script>
    }


    @RenderSection("scripts", false)

  <div id="BaseUrl" data-baseurl="@Url.Content("~")"></div>


  <!-- <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyB4NT5_bn3ZPTrYW2Il5YLaBrMHGjzjcjM&callback=initMap" async defer></script> -->

  </body>

</html>
